					<Panel hittest="false" id="TeamLogo" />
					<Label hittest="false" id="TeamScore" text="999" />
				</Panel>
			</Panel>
		</Panel>
		<Panel hittest="false" id="TeamOverlayXMLFile" />
	</Panel>
</root>